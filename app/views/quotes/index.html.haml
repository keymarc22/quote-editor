= turbo_stream_from current_company, "quotes"

%main.container
  = turbo_frame_tag "first_turbo_frame" do
    %header.header
      %h1 Quotes
      = link_to "New quote", new_quote_path, class: "btn btn--primary", data: { "turbo-frame": dom_id(Quote.new) }

  = turbo_frame_tag dom_id(Quote.new)

  = turbo_frame_tag 'quotes' do
    = render "quotes/empty_state"
    = render @quotes